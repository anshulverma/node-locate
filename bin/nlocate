#!/usr/bin/env node

(function() {
  require('coffee-script/register');
  var argv = require('minimist')(process.argv.slice(2));

  var locate = require('../lib/main');
  var name = argv['_'][0];
  try {
    var cmd = locate(name);
    console.log(cmd);
  } catch (err) {
    console.error(err.message);
    process.exit(1)
  }
}).call(this);
